<idea-plugin>
  <id>org.example.groupwork</id>
  <name>groupwork ch-&gt;en</name>
  <version>2020.3.1</version>
  <vendor email="support@yourcmpany.com" url="http://www.yorcompany.com">Group10</vendor>
  <description>更改中文字符为英文字符，因为至少40个字所以11111111111111111111111111111111111111111111111111111</description>
  <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
  <idea-version since-build="213.5449" until-build="213.*" />
  <depends>com.intellij.modules.platform</depends>
  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <applicationConfigurable instance="CnCharSettingComponent" />
    <!--        <applicationService serviceInterface="" serviceImplementation="applicationImpl"/>-->
  </extensions>
  <actions>
    <!-- Add your actions here -->
    <action id="myAction" class="MyAction">
      <add-to-group group-id="GenerateGroup" anchor="last" />
    </action>
  </actions>
</idea-plugin>
